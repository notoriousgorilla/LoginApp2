<div>
   
   <h1>Sign Up</h1>
   <%= form_for(@user) do |f| %>
    <div>
        <p>first name</p><%= f.text_field :first_name %> 
        <p>last name</p><%= f.text_field :last_name %> 
        <p>email</p><%= f.text_field :email %>
        <p>password</p><%= f.password_field :password %>
    </div>
    <div>
        <%= f.submit "create" %>
    </div>
    <% end %>
    
</div>

